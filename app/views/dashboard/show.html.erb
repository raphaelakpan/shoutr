<h1>Dashboard</h1>

<%= shout_form_for(TextShout) do |form| %>
  <%= form.text_field :body, placeholder: "What's on your mind?", required: true %>
<% end %>

<%= shout_form_for(PhotoShout) do |form| %>
  <%= form.file_field :image, required: true %>
<% end %>

<%= render @dashboard_presenter.timeline %>
